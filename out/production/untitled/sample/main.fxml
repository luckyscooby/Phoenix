                                                        </CheckBox>
                                                        <CheckBox mnemonicParsing="false" selected="true" text="Show log when done" />
                                                        <CheckBox mnemonicParsing="false" selected="true" text="Enable &quot;On The Fly&quot; (do not shutdown/lock database)" />
                                                    </children>
                                                </VBox>
                                            </content>
                                        </TitledPane>
                                        <Button layoutX="206.0" layoutY="326.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="100.0" text="Backup Now" textFill="#0083c9" />
                                    </children>
