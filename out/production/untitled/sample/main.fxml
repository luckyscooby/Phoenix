                                        <ProgressBar layoutX="10.0" layoutY="361.0" prefHeight="18.0" prefWidth="501.0" progress="0.0">
                                            <cursor>
                                                <Cursor fx:constant="DEFAULT" />
                                            </cursor>
                                        </ProgressBar>
                                        <TextField fx:id="dbPathField2" layoutX="80.0" layoutY="10.0" prefHeight="25.0" prefWidth="300.0" promptText="File path" />
